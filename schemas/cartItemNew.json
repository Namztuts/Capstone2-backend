{
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/cart_item.schema.json",
   "type": "object",
   "properties": {
      "id": {
         "type": "integer",
         "minimum": 1
      },
      "cart_id": {
         "type": "integer",
         "minimum": 1
      },
      "product_id": {
         "type": "integer",
         "minimum": 1
      },
      "quantity": {
         "type": "integer",
         "minimum": 1
      }
   },
   "required": ["cart_id", "product_id", "quantity"],
   "additionalProperties": false
}
